                    <div id="msform">
                        <!-- progressbar -->



                        <fieldset>
                            <h3 class="fs-subtitle" style="margin-bottom:1px;">
                                <span>Help Us with your Details </span>
                            </h3>

                            <div class="row-health">
                                @*<h5><span>Tell us about you</span></h5>*@
                                <h6><span>Gender</span></h6>
                                <div class="middle">
                                    <label>


                                        @Html.RadioButtonFor(model => model.Gender, "Male")
                                        @*  <input class="gndre" type="radio" id="rdbmale" name="radio1" checked />*@
                                        <div class="front-end box">
                                            <span>Male</span>
                                        </div>
                                    </label>

                                    <label>
                                        @Html.RadioButtonFor(model => model.Gender, "Female")
                                        @*<input class="gndre" type="radio" id="rdbfemale" name="radio1" />*@
                                        <div class="back-end box">
                                            <span>Female</span>
                                        </div>
                                    </label>
                                </div>

                                <div class="middle-two">
                                    <div>

                                        <label class="healthlbl col-xs-12 col-sm-12 col-md-6">
                                            <p class="label-txt"><i class="fa fa-user"></i>Full Name</p>
                                            @Html.TextBoxFor(model => model.Full_Name, new { @class = "healthinpt", @autocomplete = "off" })
                                            <span class="error1" style="color: #d25068; font-size:13px; display: none"> * Only Alphabets digits</span>
                                        </label>



                                    </div>
                                    <div>

                                        <label class="healthlbl col-xs-12 col-sm-12 col-md-6">
                                            <p class="label-txt"><i class="fa fa-phone-square"></i>Mobile No</p>
                                            @Html.TextBoxFor(model => model.MobileNo, new { @class = "healthinpt", @maxlength = "10", @minlength = "10", @autocomplete = "off" })
                                            <span class="error" style="color: #d25068; font-size:13px;  display: none">* Mobile No Input digits (0 - 9)</span>
                                        </label>



                                    </div>

                                </div>
                            </div>
                            <input type="text" class="next action-button btn-primary-ins btn-primary-insbtnerd" id="savedre" placeholder="Proceed" />


                            <div style="margin: 1% 1% 2%; font-weight: bold; text-align: center; font-size: 12px;">
                                <span>By clicking you agree to our <a href="/PDF/CYP_Privacy%20Policy_Release.pdf" target="_blank">Privacy policy</a> & <a href="/PDF/CYP%20-%20Terms%20and%20Conditions_Release.pdf" target="_blank">Terms & Conditions</a></span>
                                <span style="display:block;width:100%;text-align-last:right;font-size:9px;color: #676767;">*(Standard T&C Apply)</span>
                            </div>
                        </fieldset>










                        <fieldset>
                            <h3 class="fs-subtitle">
                                <span>Find the right health plan for you in </span>
                            </h3>

                            <hr>

                            <div style="float: left;width:100%;">
                                <div class="col-xs-4 col-sm-5 col-md-5" style="padding:0px;">
                                    <h6 class="bcvgd">Who would you<br>Like to Insure</h6>
                                </div>


                                <div class="col-xs-8 col-sm-7 col-md-7" style="padding:0px;">
                                    <label class="healthlbl gbh col-xs-4 col-sm-12 col-md-2">
                                        <span class="label-gb">Self</span>
                                        @Html.CheckBoxFor(model => model.chkself)
                                        <span class="error" style="color: #d25068; font-size:13px;  display: none">* Mobile No Input digits (0 - 9)</span>
                                    </label>


                                    <label class="healthlbl gbh col-xs-4 col-sm-12 col-md-2">
                                        <span class="label-gb">Spouse</span>
                                        @Html.CheckBoxFor(model => model.chkspouses)
                                        <span class="error" style="color: #d25068; font-size:13px;  display: none">* Mobile No Input digits (0 - 9)</span>
                                    </label>
                                    <label class="healthlbl gbh col-xs-4 col-sm-12 col-md-2">
                                        <span class="label-gb">Child</span>
                                        @Html.TextBoxFor(model => model.son, new { @type = "number" })
                                        <span class="error" style="color: #d25068; font-size:13px;  display: none">* Mobile No Input digits (0 - 9)</span>
                                    </label>

                                </div>
                            </div>

                            <div style="float: left;padding-top:5%;">
                                <div class="col-xs-4 col-sm-5 col-md-5">
                                    <h6 class="bcvgd">AGE OF<br>Person(S)</h6>
                                </div>
                                <div class="col-xs-8 col-sm-7 col-md-7" style="padding:0px;">
                                    <div style="float:left;">
                                        <div class="col-xs-6 col-sm-3 col-md-4">
                                            <label for="ageSelf" class="ageSelf">

                                                @Html.TextBoxFor(model => model.ageSelf, new { @placeholder = " " })

                                                @*<input type="text" id="inp" placeholder="&nbsp;">*@
                                                @*@Html.TextBoxFor(model => model.ageSelf)*@

                                                <span class="label">Self </span>
                                                @Html.ValidationMessageFor(model => model.ageSelf, "", new { @class = "field-validation-error" })
                                            </label>
                                        </div>

                                        <div class="col-xs-6 col-sm-3 col-md-4">
                                            <label for="ageSpouse" class="ageSpouse">
                                                @Html.TextBoxFor(model => model.ageSpouse, new { @placeholder = " " })
                                                <span class="label">Spouse</span>

                                            </label>
                                        </div>


                                        <div class="col-xs-6 col-sm-3 col-md-4">
