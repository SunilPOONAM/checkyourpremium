<script>
    function BindPremium() {
        var fullname = $("#hdnTrnasId").val();
       // alert(fullname);
        $('#mytable').dataTable().fnDestroy();
        var _searchData = "";
        var dataTable = $('#mytable').DataTable({
            "responsive": true,
            "xscroll": true,
            "paging": false,

            "searching": false,
            "stateSave": false,
            "ordering": true,
            "order": [[2, "desc"]],
            "info": false,
            "oLanguage": {
                "sEmptyTable": "No record"
            },
            "lengthMenu": [[10, 25, 50, 100, -1], [10, 25, 50, 100, "All"]],
            "processing": true,
            "serverSide": true,
            "searching": false,
            "lengthChange": false,
            "ajax": {
                url: "/Customer/SearchdataHealth", // json datasource
                type: "Post",  // method  , by default get AgeAdult, string Self, string Adult, string Child ageSpouse,agechild1,agechild2

                data: { "searchTxt": _searchData, "FullName": fullname },

                error: function () {  // error handling
                    jQuery("#example1").append('<tbody class="business-user-grid-error"><tr><th colspan="4">No record</th></tr></tbody>');

                }
            },




            "createdRow": function (row, data, rowIndex) {
                $(row).find('td:eq(0)').attr('data-label', 'Full Name');
                $(row).find('td:eq(1)').attr('data-label', 'Gender');
                $(row).find('td:eq(2)').attr('data-label', 'Mobile No');
                $(row).find('td:eq(3)').attr('data-label', 'City');
                $(row).find('td:eq(4)').attr('data-label', 'Downlode Policy');
                $(row).find('td:eq(5)').attr('data-label', 'Payment Status');
                $(row).find('td:eq(6)').attr('data-label', 'Purchage Date');
            },


            "columnDefs": [{
                "targets": 0,
                "orderable": false,
                "sClass": "dataTableCenter"
            }, {
                "targets": 2,
                "orderable": false,
                "sClass": "left"
            }]
        });
        return dataTable;
    }
</script>
