                    url: "/TermAndLife/GetKotakPlan",
                    method: "get",
                    data: { "ptype": ptype },
                    success: function (result) {
                        $('#PolicyIndiaFirst option').remove();
                        $.each(result, function () {
                            $("#PolicyIndiaFirst").append($("<option></option>").val(this['Value']).html(this['Text']));

                        });

                    }
                })
                if (ptype == "Limited Pay") {
                    $("#pptbox").css('display', 'block');
